<nav class="{{ mix }} cs-navigation">
    <ul class="cs-navigation__list">
        {% for item in items %}
            <li class="{{ item.mix }} cs-navigation__item" data-nav-group="{{ item.dataNavGroup }}">
                <a href="{{ item.url }}" class="cs-navigation__link">
                    {{ item.label }}
                </a>
                {% if item.hasChildren %}
                    <div class="cs-navigation__categories" data-nav-group="{{ item.dataNavGroup }}">
                        {% for item in item.subcategories %}

                            <div class="cs-navigation__subcategory">
                                <a href="{{ item.url }}" class="cs-navigation__subcategory-label">{{ item.label }}</a>
                                <ul class="cs-navigation__subcategory-list">
                                    {% for item in item.subcategories %}
                                        <li class="cs-navigation__subcategory-item">
                                            <a href="item.url" class="cs-navigation__subcategory-link">{{ item.label }}</a>
                                        </li>
                                    {% endfor %}
                                </ul>
                            </div>

                        {% endfor %}
                    </div>
                {% endif %}
            </li>
        {% endfor %}
    </ul>
</nav>