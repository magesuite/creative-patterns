<ul class="{{ mix }} cs-size-picker">
    {% for item in items %}
    <li class="cs-size-picker__item">
        <input name="{{ name }}" id="{{ item.id }}" class="cs-size-picker__input" type="radio">
        <label for="{{ item.id }}" class="cs-size-picker__label">{{ item.value }}</label>
    </li>
    {% endfor %}
</ul>