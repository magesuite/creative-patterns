<ul class="{{ mix }} cs-social-icons">
    {% for item in items %}

        <li class="{{ item.mix }} cs-social-icons__item">
            <a href="{{ item.url }}" class="cs-social-icons__link">
                <svg class="cs-social-icons__icon">
                    <use xlink:href="../images/sprites.svg#{{ item.iconID }}"></use>
                </svg>
            </a>
        </li>

    {% endfor %}
</ul>