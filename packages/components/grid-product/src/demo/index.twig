        <script src="https://code.jquery.com/jquery-1.12.4.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.18.1/moment.min.js"></script>
        <script src="../../../item-cloner/dist/item-cloner.js"></script>

        <script>
            function addNewCallbackOutput( color, text ) {
                $( '#item-cloner-output' ).prepend( '<li style="color: '+color+'"><span>'+text+'</span> <span>time: '+moment().format('H:mm:ss:SSS')+'</span></li>' );
            }

            new itemCloner( $( '.cs-grid-product:not(.touch-hover-disabled)' ), {
                originHoverClass: 'cs-grid-product--hidden',
                cloneContentHoverClass: 'cs-grid-product--show-details',
            } );

            new itemCloner( $( '.item-cloner-example' ), {
                originHoverClass: 'cs-grid-product--hidden',
                cloneContentHoverClass: 'cs-grid-product--show-details',
                touch: {
                    enabled: false,
                },
                onBeforeShow: function( cloner ) {
                    addNewCallbackOutput( 'blue', 'onBeforeShow' );
                },
                onShown: function( cloner ) {
                    addNewCallbackOutput( 'green', 'onShown' );
                },
                onBeforeHide: function( cloner ) {
                    addNewCallbackOutput( 'orange', 'onBeforeHide' );
                },
                onHidden: function( cloner ) {
                    addNewCallbackOutput( 'purple', 'onHidden' );
                },
            } );

            new itemCloner( $( '.item-cloner-example-addons-visible' ), {
                originHoverClass: 'cs-grid-product--hidden',
                cloneContentHoverClass: 'cs-grid-product--show-details',
                touch: {
                    enabled: false,
                    displayAsStatic: true,
                    staticClass: 'cs-grid-product--static',
                },
            } );

            new itemCloner( $( '.item-cloner-static-below-breakpoint' ), {
                originHoverClass: 'cs-grid-product--hidden',
                cloneContentHoverClass: 'cs-grid-product--show-details',
                belowBreakpointClass: 'cs-grid-product--static',
            } );
        </script>
