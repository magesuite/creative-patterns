<form class="{{ mix }} cs-search-field">
    <input class="cs-search-field__input" id="{{ fieldId }}" name="fieldName">
    <label for="{{ fieldId }}" class="cs-search-field__label">{{ label }}</label>
    <button type="submit" class="cs-search-field__button">
        <svg class="cs-search-field__icon">
            <use xlink:href="../images/sprites.svg#{{ iconId }}"></use>
        </svg>
    </button>
</form>